{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react",
        "baseUrl": "./src",

        "strict": true,
        "alwaysStrict": true,
    
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,

        "strictNullChecks": false,
        "noImplicitAny": false
    }
}
